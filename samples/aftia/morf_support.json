{
  "config": {
    "submit": "https://prod-29.canadacentral.logic.azure.com:443/workflows/a8bb0fd18d624316af0e4e2513267d83/triggers/manual/paths/invoke?api-version=2016-06-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=WSegwPOJvIeE1mnu9vl-sJFJZCm6p3BGpK-H-cJXrEg",
    "siteKey": "4fbfd303f2f74c6ea2ec3bc9ab44d454",
    "successUrl": "",
    "theme": "",
    "externalId": ""
  },
  "head": {
    "title": "Aftia Labs: Product Support Incident Report",
    "logo": "https://uploads-ssl.webflow.com/61e714dee6e03a006b829c3a/621cf6cde8ae4f61b08896b4_MORF%20Logo.svg"
  },
  "body": {
    "items": [
      {
        "type": "panel",
        "width": "full",
        "label": "Contact Information",
        "items": [
          {
            "type": "text",
            "label": "Organization Name",
            "width": "full",
            "placeholder": "E.g. Vandelay Industries",
            "required": true,
            "bind": "contact.org"
          },
          {
            "type": "text",
            "width": "full",
            "label": "Contact Name",
            "placeholder": "E.g. Jane Doe",
            "required": true,
            "bind": "contact.name"
          },
          {
            "type": "email",
            "width": "half",
            "label": "Email",
            "placeholder": "E.g. me@mycompany.com",
            "required": true,
            "bind": "contact.email"
          },
          {
            "type": "tel",
            "width": "half",
            "label": "Phone",
            "placeholder": "E.g. 1-555-555-1234",
            "required": true,
            "bind": "contact.phone"
          }
        ]
      },
      {
        "type": "panel",
        "width": "full",
        "label": "Incident Information",
        "items": [
          {
            "type": "text",
            "label": "Summary",
            "bind": "incident.summary",
            "required": true
          },
          {
            "type": "textarea",
            "width": "full",
            "label": "Describe the Issue",
            "bind": "incident.issue",
            "required": true
          },
          {
            "type": "textarea",
            "width": "full",
            "label": "Expected Result or Resolution",
            "bind": "incident.expresult",
            "required": true
          },
          {
            "type": "dropdown",
            "bind": "incident.severity",
            "label": "Issue Severity",
            "options": [
              {
                "label": "1-Critical",
                "value": "1"
              },
              {
                "label": "2-Urgent",
                "value": "2"
              },
              {
                "label": "3-Important",
                "value": "3"
              },
              {
                "label": "4-Minor",
                "value": "4"
              }
            ],
            "required": true
          },
          {
            "type": "file",
            "label": "Add Attachment",
            "bind": "incident.attachments",
            "required": false
          }
        ]
      },
      {
        "type": "submit",
        "label": "Submit"
      }
    ]
  }
}